{
    "name": "Expo React Native",
    "dockerFile": "./Dockerfile",
    "context": "..",
    "workspaceFolder": "/home/node/app",
    "forwardPorts": [
        19000,
        19001,
        19002,
        8081
    ],
    "appPort": 8081,
    "remoteUser": "node",
    "postCreateCommand": "npm install",
    "customizations": {
        "vscode": {
            "extensions": [
                "dbaeumer.vscode-eslint",
                "esbenp.prettier-vscode",
                "ms-vscode.vscode-react-native"
            ]
        }
    },
    "runArgs": [
        "-it",
        "--rm"
    ],
    "overrideCommand": false

}